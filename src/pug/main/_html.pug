extends ../core/_html

block append vars
  include _functions
  include _vars
  include _logo
  include _nav
  - layout = 'main/_html'
  - INCLUDE_THEMEJS = true

block stylesheet
  +cssv(homeUri+'css/main.css')#_style
  +cssv(homeUri+'css/custom.css')#_custom_style

block prepend style
  +logo_style

block footer
  footer
    hr._m0
    ._container: small| &copy; !{year} !{author}
